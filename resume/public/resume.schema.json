{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Resume Data Schema",
  "description": "Schema for Jiqing Yang's resume data structure",
  "type": "object",
  "properties": {
    "personal": {
      "type": "object",
      "properties": {
        "firstName": { "type": "string" },
        "lastName": { "type": "string" },
        "email": { "type": "string", "format": "email" },
        "phone": { "type": "string" },
        "location": { "type": "string" },
        "website": { "type": "string", "format": "uri" },
        "github": { "type": "string" },
        "linkedin": { "type": "string" }
      },
      "required": ["firstName", "lastName", "email"]
    },
    "education": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "institution": { "type": "string" },
          "degree": { "type": "string" },
          "minors": {
            "type": "array",
            "items": { "type": "string" }
          },
          "location": { "type": "string" },
          "startDate": { "type": "string" },
          "endDate": { "type": ["string", "null"] },
          "gpa": { "type": "string" }
        },
        "required": ["institution", "degree", "location", "startDate"]
      }
    },
    "experience": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "organization": { "type": "string" },
          "location": { "type": "string" },
          "startDate": { "type": "string" },
          "endDate": { "type": ["string", "null"] },
          "description": { "type": "string" },
          "bullets": {
            "type": "array",
            "items": { "type": "string" }
          },
          "tags": {
            "type": "array",
            "items": { "type": "string" }
          }
        },
        "required": ["title", "organization", "startDate"]
      }
    },
    "projects": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "date": { "type": "string" },
          "status": {
            "type": "string",
            "enum": ["active", "completed", "archived", "unmaintained"]
          },
          "github": { "type": "string" },
          "liveUrl": { "type": ["string", "null"], "format": "uri" },
          "description": { "type": "string" },
          "bullets": {
            "type": "array",
            "items": { "type": "string" }
          },
          "tags": {
            "type": "array",
            "items": { "type": "string" }
          },
          "featured": { "type": "boolean" }
        },
        "required": ["title", "date", "status"]
      }
    },
    "extracurricular": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "type": {
            "type": "string",
            "enum": ["competition", "honor", "club", "other"]
          },
          "organization": { "type": "string" },
          "website": { "type": ["string", "null"], "format": "uri" },
          "dates": { "type": "string" },
          "achievements": {
            "type": "array",
            "items": { "type": "string" }
          },
          "description": { "type": "string" },
          "tags": {
            "type": "array",
            "items": { "type": "string" }
          }
        },
        "required": ["title", "type"]
      }
    }
  },
  "required": ["personal", "education", "experience", "projects", "extracurricular"]
}
